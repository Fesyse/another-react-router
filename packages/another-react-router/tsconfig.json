{
	"compilerOptions": {
		"jsx": "react",
		"target": "ES2023",
		"module": "ESNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"noEmit": true,
		"moduleResolution": "node",
		"noUncheckedIndexedAccess": true,
		"paths": {
			"@/*": ["./src/*"]
		}
	},
	"include": ["src"],
	"exclude": ["./src/another-react-router.config.ts"]
}
